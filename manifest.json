{
    "manifest_version": 3,
    "name": "Social Media Brainteasers",
    "description": "A social media timer which generates brainteasers.",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "brainteaserIcon.png"
    },
    "background": {
      "service_worker": "timer.js"
    },
    "permissions": [
      "tabs", "notifications"
    ]
  }